(+ 1 1)

(define (square x) (* x x))

;;sum of squares
(define (sum-of-squares x y) (+ (square x) (square y)))

;;case analysis
(define (abs x)
  (cond ((> x 0) x)
	((= x 0) 0)
	((< x 0) (- x))))
